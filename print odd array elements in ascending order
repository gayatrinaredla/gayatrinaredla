import java.util.*;
class set
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
System.out.println("Enter size");
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
{
int b=s.nextInt();
a[i]=b;
}
Arrays.sort(a);
for(int j=0;j<n;j++)
{
if(a[j]%2!=0)
{
System.out.println(a[j]);
}}
}
}
